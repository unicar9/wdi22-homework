<h2>All mountains</h2>

<% @mountains.each do |mountain| %>
<p>
  <a href="/mountains/<%= mountain["id"] %>"><%= mountain["name"] %></a>
  <a href="/mountains/<%= mountain["id"] %>/edit"   class="edit" >edit</a>
 <a href="/mountains/<%= mountain["id"] %>/delete"   class="delete" >delete</a>
</p>
<% end %>
